{
	"name": "vlmm-worker",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"test": "vitest",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"db:seed": "wrangler d1 execute vlm-api-db --file=./seeds/0001_seed_prompts.sql",
		"db:export": "wrangler d1 export vlm-api-db --output=./d1-exports/dump.sql",
		"db:import": "wrangler d1 execute vlm-api-db --file=./d1-exports/dump.sql",
		"cf-typegen": "wrangler types"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.32.0"
	}
}
